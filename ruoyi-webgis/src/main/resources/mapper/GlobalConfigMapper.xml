<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.webgis.mapper.GlobalConfigMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ruoyi.webgis.model.po.GlobalConfigPO">
        <id column="config_id" property="configId" />
        <result column="config_name" property="configName" />
        <result column="config_value" property="configValue" />
        <result column="del_tag" property="delTag" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="remark" property="remark" />
        <result column="server_name" property="serverName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        config_id, config_name, config_value, del_tag, create_time, update_time, remark, server_name
    </sql>

</mapper>
